# Example environment variables (do NOT commit real secrets)

# MySQL (SQL Database)
SPRING_DATASOURCE_URL=jdbc:mysql://host.docker.internal:3306/keypointhub
SPRING_DATASOURCE_USERNAME=<your_mysql_username>
SPRING_DATASOURCE_PASSWORD=<your_mysql_password>

# MongoDB (GridFS)
SPRING_DATA_MONGODB_URI=mongodb://host.docker.internal:27017/keypointhub_images

# Mail (SMTP)
SPRING_MAIL_HOST=smtp.gmail.com
SPRING_MAIL_PORT=587
SPRING_MAIL_USERNAME=<your_smtp_username>
SPRING_MAIL_PASSWORD=<your_smtp_password_or_app_password>
REPORT_MAIL_TO=<recipient@example.com>

# External services
MEDIAPIPE_SERVICE_URL=http://host.docker.internal:8000/extract-pose

# CORS
APP_CORS_ALLOWED_ORIGINS=http://localhost:3000

